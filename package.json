{
  "name": "dashboard",
  "private": true,
  "version": "0.3.0",
  "gitHooks": {
    "pre-commit": "tsc --noEmit && pnpm lint",
    "pre-push": "pnpm rsc"
  },
  "scripts": {
    "start": "cross-env VITE_APP_ENVIRONMENT=development vite",
    "build": "cross-env VITE_APP_ENVIRONMENT=production tsc --noEmit && vite build",
    "analyze": "cross-env VITE_APP_ENVIRONMENT=analyze pnpm vite build",
    "preview": "vite preview",
    "lint": "pnpm lint:styles && pnpm lint:scripts",
    "lint:styles": "stylelint \"src/**/*.{css,sass,scss}\" --max-warnings=0",
    "lint:scripts": "eslint \"{src,config}/**/*.{js,ts,jsx,tsx}\" --cache --fix --max-warnings=0",
    "rsc": "node scripts/release-sanity-check.mjs",
    "codegen-generate": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env"
  },
  "dependencies": {
    "@apollo/client": "^3.7.2",
    "@cosmjs/amino": "^0.29.5",
    "@cosmjs/encoding": "^0.29.5",
    "@cosmjs/launchpad": "^0.27.1",
    "@cosmjs/proto-signing": "^0.29.5",
    "@cosmjs/stargate": "0.27.0",
    "@distributedlab/tools": "^0.1.7",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hookform/resolvers": "^2.9.10",
    "@mui/material": "^5.11.4",
    "@rarimo/client": "0.1.1",
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^0.27.2",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "i18next": "^22.4.3",
    "iconoir-react": "^6.0.0",
    "jdenticon": "^3.2.0",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.8.1",
    "mitt": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.41.0",
    "react-i18next": "^12.1.1",
    "react-json-view": "^1.21.3",
    "react-number-format": "^5.1.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1",
    "recharts": "^2.3.2",
    "uuid": "^9.0.0",
    "vite-plugin-svgr": "^2.4.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@comparaonline/stylelint-config-scss-modules": "^1.1.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/client-preset": "^1.2.3",
    "@graphql-codegen/typescript-document-nodes": "^2.3.10",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@keplr-wallet/types": "^0.11.23",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "chalk": "^5.2.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.16.4",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-i18n": "^1.0.3",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "^1.56.2",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-declaration-strict-value": "^1.9.1",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vite-plugin-checker": "^0.5.3",
    "vite-tsconfig-paths": "^4.0.2",
    "yorkie": "^2.0.0"
  }
}
